    <Grid>
        <StackPanel>
            <ui:CardAction Click="CardAction_Click" Icon="Fluent24">感谢 WPF UI 为本程序提供样式</ui:CardAction>
            <ui:TextBox IsReadOnly="True" x:Name="text" ></ui:TextBox>
        </StackPanel>
   </Grid>
